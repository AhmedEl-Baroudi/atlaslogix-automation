			"item": [
				{
					"name": "v1 auth login create",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"//Automatic check: validate that token is saved as a collection variable",
									"pm.test('Login returns token', function () {",
									"  pm.response.to.have.status(200);",
									"  const errorStatuses = [400, 401, 403, 404, 409, 500];",
									"  pm.expect(errorStatuses).to.not.include(pm.response.code);",
									"  const json = pm.response.json();",
									"  pm.expect(json.token).to.be.a('string');",
									"});",
									"",
									"//Save Token & Tenant ID (If Exists) as Collection Variables",
									"try {",
									"  const json = pm.response.json();",
									"  if (json.token) {",
									"    pm.collectionVariables.set('token', json.token);",
									"    if (json.tenantId) pm.collectionVariables.set('tenantId', json.tenantId);",
									"  }",
									"} catch (e) {}",
									"",
									"//Extra validation: confirm token is stored in collection variables",
									"pm.test(\"Token saved to collection variable\", function () {",
									"  const savedToken = pm.collectionVariables.get(\"token\");",
									"  pm.expect(savedToken).to.be.a(\"string\").and.not.empty;",
									"});",
									"",
									"//Schema/field existence checks (key fields in login response)",
									"pm.test(\"Login response has required fields\", function () {",
									"  const json = pm.response.json();",
									"  pm.expect(json).to.have.property(\"token\").and.to.be.string;",
									"  pm.expect(json).to.have.property(\"role\").and.to.be.string;",
									"  pm.expect(json).to.have.property(\"tenantId\").and.to.be.string;",
									"  pm.expect(json).to.have.property(\"userId\").and.to.be.string;",
									"});",
									"",
									"//Basic response-time check (threshold: 1500ms)",
									"pm.test(\"Response time is acceptable\", function () {",
									"  pm.expect(pm.response.responseTime).to.be.below(1500);",
									"});",
									""
								],
								"type": "text/javascript"
							}
						}
					],
